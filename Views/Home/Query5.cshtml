@model IEnumerable<KickerShop.Models.Report>

@{
    ViewBag.Title = "Query5";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Raport czasowy</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Jare)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Total)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Order_Count)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Avg_Per_Order)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Jare)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Total)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Order_Count)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Avg_Per_Order)
        </td>
    </tr>
}

</table>
